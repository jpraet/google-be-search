{
	"name": "Google BE Search",
	"description": "Add google.be to your search engines (including search suggestions from google.be).",
	"version": "0.1",
	"manifest_version": 2,
	"applications":{
		"gecko": {
			"id": "google-be-search@jpraet.github.io",
			"strict_min_version": "64.0"
		}
	},
	"developer": {
		"name": "Jimmy Praet",
		"url": "https://github.com/jpraet/"
	},
	"icons": {
		"256": "favicon.png"
	},
	"web_accessible_resources": [
		"favicon.png"
	],
	"chrome_settings_overrides": {
		"search_provider": {
			"name": "Google BE",
			"search_url": "https://www.google.be/search?q={searchTerms}",
			"suggest_url": "https://www.google.be/complete/search?client=firefox&hl=en-GB&q={searchTerms}"
		}
	}
}
